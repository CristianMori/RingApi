<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>KoenZomers.Ring.Api</RootNamespace>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\KoenZomers.Ring.Api.snk</AssemblyOriginatorKeyFile>
    <AssemblyName>KoenZomers.Ring.Api</AssemblyName>
    <Version>0.3.2.0</Version>
    <Company>Koen Zomers</Company>
    <Authors>Koen Zomers</Authors>
    <Description>API in .Net Standard to communicate with the Ring Doorbell service</Description>
    <PackageProjectUrl>https://github.com/KoenZomers/RingApi</PackageProjectUrl>
    <AssemblyVersion>0.3.2.0</AssemblyVersion>
    <PackageReleaseNotes>- Added support for working with Ring Stickup Cams
- Removed CredentialsEncoded property in Session as it was no longer used and deprecated a few versions back</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Copyright>Koen Zomers</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\KoenZomers.Ring.Api.snk" Link="KoenZomers.Ring.Api.snk" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
  </ItemGroup>

</Project>
